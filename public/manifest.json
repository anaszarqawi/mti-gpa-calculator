{
  "manifest_version": 3,
  "name": "MTI GPA Calculator",
  "description": "All you have to do is show me your grades ;)",
  "author": "info@anaszarqawi.com",
  "version": "1.0",
  "icons": {
    "16": "logo16.png",
    "32": "logo32.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "permissions": ["activeTab", "scripting"],
  "content_scripts": [
    {
      "js": ["js/content_script.js"],
      "matches": ["http://www.mti.edu.eg/university/student/*"]
    }
  ]
}
